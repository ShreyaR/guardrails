<?xml version="1.0"?>
<prompt>
    <string name='interest_rates' description='What are the interest rates offered by the bank on savings and checking accounts, loans, and credit products?'/>
    <list name="fees" description="What fees and charges are associated with my account?">
        <item type="object">
            <key name="index" type="int" format="1-indexed" />
            <key name="name" type="string" format="lower-case two-words-only" />
            <key name="explanation" type="string" format="one-line" />
            <key name="value" type="float | int" format="percentage" />
            <key name="description" type="string" format="less-than-200-characters" />
            <key name="example" type="string" required="True" format="tone-twitter explain-high-quality" />
            <key name="advertisement" type="string" format="tagline tv-ad" />
        </item>
    </list>
</prompt>




<?xml version="1.0"?>
<prompt>
    <interest_rates type='string' description='What are the interest rates offered by the bank on savings and checking accounts, loans, and credit products?'/>
    <fees type="list" description="What fees and charges are associated with my account?">
        <item type="object">
            <index type="int" format="1-indexed" />
            <name type="string" format="lower-case two-words-only" />
            <explanation type="string" format="one-line" />
            <value type="float | int" format="percentage" />
            <description type="string" format="less-than-200-characters" />
            <example type="string" required="True" format="tone-twitter explain-high-quality" />
            <advertisement type="string" format="tagline tv-ad" />
        </item>
    </fees>
</prompt>






interface Response {
  interest_rates: $string;
  fees: $list; // only return 4 items
}

interface $list {
  index: $int;
  name: $string;
  explanation: $string;
  value: $float | $int;
  description: $string;
  example: $string;
  advertisement: $string;
}